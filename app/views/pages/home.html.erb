<h1><%="Hi, " + current_user.first_name if current_user %></h1>
<p>Find me in app/views/pages/home.html.erb</p>
<h1>Blog posts</h1>
<table>
  <tbody>
    <% @pages.each do |page| %>
      <td><%= page.title %></td>
    <% end %>
  </tbody>
</table>
<h1>Skills</h1>
<% @skills.each do |skill| %>
  <p><%= skill.title %>: <%=skill.percent_utilized %> percents</p>
<% end %>
